class Solution {
  public:
    vector<int> rearrange(vector<int>& arr) {
        // Code here
        unordered_map<int,bool>mp;
        int n = arr.size();
        for(auto num : arr){
            if(num>=0){
                mp[num] = true;
            }
        }
        for(int i = 0; i<arr.size(); i++){
            if(mp[i]){
                arr[i] = i;
            }else{
                 arr[i] = -1;
            }
        }
        return arr;
    }
};
