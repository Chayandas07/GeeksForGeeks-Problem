class Solution{
public:
    int LargButMinFreq(int arr[], int n) {
        // code here
        map<int,int> mp;
        vector<int> ans;
        for(int i=0;i<n;i++){
            mp[arr[i]]++;
        }
        int minu=INT_MAX;
        for(auto it: mp){
            if(it.second<=minu){
                minu=it.second;
                ans.push_back(it.first);
            }
        }
        int result=*max_element(ans.begin(), ans.end());
        return result;
    }
};
